{
  "name": "tailor-content-element",
  "private": true,
  "description": "Content element starter template",
  "author": "ExtensionEngine <info@extensionengine.com>",
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/content-element.umd.cjs",
  "module": "./dist/content-element.js",
  "exports": {
    ".": {
      "import": "./dist/content-element.js",
      "require": "./dist/content-element.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "setup": "node src/setup/index.cjs",
    "lint:js": "eslint --ext .js,.ts,.svelte .",
    "lint:css": "stylelint \"src/**/*.svelte\" \"src/**/*.scss\" \"src/**/*.css\"",
    "lint": "npm run lint:js && npm run lint:css"
  },
  "bin": {
    "tailor-content-element": "bin/index.cjs"
  },
  "devDependencies": {
    "@extensionengine/eslint-config": "^3.0.1",
    "@extensionengine/stylelint-config": "^1.1.1",
    "@npmcli/package-json": "^2.0.0",
    "@smui/layout-grid": "^6.0.0",
    "@smui/top-app-bar": "^6.0.0",
    "@sveltejs/vite-plugin-svelte": "^1.0.1",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "chalk": "^4.1.2",
    "enquirer": "^2.3.6",
    "eslint": "^8.21.0",
    "eslint-config-semistandard": "^17.0.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "shelljs": "^0.8.5",
    "stylelint": "^13.13.1",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-material-ui": "^6.0.0",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.0"
  }
}
